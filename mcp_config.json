{
    "mcpServers": {
        "weather_service": {
          "command": "uv",
          "args": [
            "run",
            "./weather/main.py"
          ],
          "env": {
            "API_KEY": ""
          }
        },
        "time_service": {
            "command": "uv",
            "args": [
              "run",
              "./time/main.py"
            ]
          },
        "echo_service": {
            "command": "uv",
            "args": [
              "run",
              "./echo/main.py"
            ]
        }
    }
}